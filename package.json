{
	"name": "o",
	"version": "1.0.0",
	"main": "app.js",
	"license": "MIT",
	"scripts": {
		"start": "yarn build-win && yarn launch-dev",
		"start-prod-linux": "yarn build-linux && yarn launch-prod",
		"start-prod-win": "yarn build-win && yarn launch-prod",
		"--": "---",
		"build-linux": "cp -rf .env.copy .env && yarn drop-build-linux && yarn drop-logs-dev-linux && tsc && mkdir -p ./storage",
		"drop-build": "rm -rf ./dist",
		"drop-logs-dev-linux": "rm -rf ./logs-dev",
		"drop-logs-prod-linux": "rm -rf ./logs-prod",
		"---": "---",
		"build-win": "echo F|Xcopy \"./.env.copy\" \"./.env\" /C /F /Q /Y && yarn drop-build-win && yarn drop-logs-dev-win && tsc && if not exist \"./storage\" mkdir \"./storage\"",
		"drop-build-win": "del /f /s /q \"./dist\"",
		"drop-logs-dev-win": "del /f /s /q \"./logs-dev\"",
		"drop-logs-prod-win": "del /f /s /q \"./logs-prod\"",
		"----": "---",
		"launch-prod": "cross-env NODE_ENV=production node -r dotenv/config ./dist/app.js",
		"launch-dev": "cross-env NODE_ENV=development node -r dotenv/config ./dist/app.js",
		"launch-test-api": "cross-env NODE_ENV=development node -r dotenv/config ./dist/test-api/test.test-api.js",
		"prettier": "prettier -w --config ./.prettierrc.json ."
	},
	"dependencies": {
		"@types/bcrypt": "^5.0.0",
		"@types/config": "^0.0.38",
		"@types/cors": "^2.8.10",
		"@types/express": "^4.17.12",
		"@types/jsonwebtoken": "^8.5.1",
		"@types/node": "^15.12.2",
		"axios": "^0.21.1",
		"bcrypt": "^5.0.1",
		"bcryptjs": "^2.4.3",
		"cors": "^2.8.5",
		"cross-env": "^7.0.3",
		"dotenv": "^10.0.0",
		"express": "^4.17.1",
		"express-validator": "^6.10.1",
		"joi": "^17.4.0",
		"jsonwebtoken": "^8.5.1",
		"nanoid": "^3.1.22",
		"pg": "^8.6.0",
		"reflect-metadata": "^0.1.13",
		"typeorm": "^0.2.32",
		"typescript": "^4.2.4",
		"winston": "^3.3.3",
		"winston-daily-rotate-file": "^4.5.5"
	},
	"devDependencies": {
		"madge": "^4.0.2",
		"prettier": "^2.3.0"
	}
}
