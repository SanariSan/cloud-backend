{
    "name": "o",
    "version": "1.0.0",
    "main": "app.js",
    "license": "MIT",
    "scripts": {
        "start": "yarn build && yarn launch-dev",
        "build": "yarn drop-build && yarn drop-logs-dev && tsc",
        "drop-build": "rm -rf ./dist",
        "drop-logs-dev": "rm -rf ./logs-dev",
        "drop-logs-prod": "rm -rf ./logs-prod",
        "launch-prod": "cross-env NODE_ENV=production node ./dist/app.js",
        "launch-dev": "cross-env NODE_ENV=development node ./dist/app.js"
    },
    "dependencies": {
        "bcrypt": "^5.0.1",
        "bcryptjs": "^2.4.3",
        "config": "^3.3.6",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "express": "^4.17.1",
        "express-validator": "^6.10.1",
        "joi": "^17.4.0",
        "jsonwebtoken": "^8.5.1",
        "nanoid": "^3.1.22",
        "pg": "^8.6.0",
        "reflect-metadata": "^0.1.13",
        "typeorm": "^0.2.32",
        "typescript": "^4.2.4",
        "winston": "^3.3.3",
        "winston-daily-rotate-file": "^4.5.5"
    },
    "devDependencies": {
        "@types/config": "^0.0.38",
        "@types/cors": "^2.8.10",
        "@types/express": "^4.17.11",
        "@types/jsonwebtoken": "^8.5.1"
    }
}
